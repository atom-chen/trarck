<idea-plugin version="2">
  <name>com.trarck.JsGetSet</name>
  <description>create javascript get set</description>
  <version>1.0</version>
  <vendor>trarck.com</vendor>

  <!-- please see http://confluence.jetbrains.net/display/IDEADEV/Build+Number+Ranges for description -->
  <idea-version since-build="9.0"/>
  <!-- if you do not specify any Depending on the plugin will only work in IntelliJ IDEA -->
  <depends>com.intellij.modules.lang</depends>
  <application-components>
    <!-- Add your application components here -->
  </application-components>

  <project-components>
    <!-- Add your project components here -->
  </project-components>

  <actions>
    <!-- Add your actions here -->
      <action id="trarck.jsgetset" class="com.trarck.JsGetSet" text="JsGetSet" description="create javascript get set ">
          <add-to-group group-id="ToolsMenu" anchor="last"/>
          <keyboard-shortcut keymap="$default" first-keystroke="shift ctrl G"/>
      </action>
  </actions>

  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
  </extensions>
  <!--<extensions defaultExtensionNs="com.strintec.intellij.webmaster">-->
    <!--&lt;!&ndash; Add your extensions here &ndash;&gt;-->
  <!--</extensions>-->

</idea-plugin>